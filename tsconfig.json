{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist/srv",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "srv/*": ["srv/*", "dist/srv/*"]
    }
  },
  "include": [
    "srv/**/*"
  ]
}
